{
  "name": "marquee-static-sdk",
  "version": "0.1.0-alpha.8",
  "scripts": {
    "prepublish": "npm run clearjs && npm run build",
    "build": "./node_modules/coffee-react/bin/cjsx --output ./ --bare --compile ./",
    "develop": "./node_modules/coffee-react/bin/cjsx --watch --output ./ --bare --compile ./",
    "clearjs": "find . -path ./node_modules -prune -or -name '*.js' | grep -v 'node_modules' | xargs rm"
  },
  "main": "./index.js",
  "bin": {
    "marqueestatic": "./bin/marqueestatic"
  },
  "dependencies": {
    "autoprefixer-core": "^3.1.2",
    "aws-sdk": "^2.0.21",
    "browserify": "^6.1.0",
    "coffee-react": "^1.0.2",
    "coffee-react-transform": "^1.0.4",
    "coffee-reactify": "^1.0.3",
    "coffee-script": "^1.8.0",
    "colors": "^1.0.3",
    "commander": "^2.5.0",
    "css": "^2.1.0",
    "fs-extra": "^0.12.0",
    "html": "0.0.7",
    "mime": "^1.2.11",
    "moment": "^2.8.3",
    "node-sass": "^1.0.1",
    "node-watch": "^0.3.4",
    "react": "^0.11.2",
    "request": "^2.45.0",
    "shiny": "^1.0.0-rc.1",
    "sqwish": "^0.2.2",
    "temp": "^0.8.1",
    "uglify-js": "^2.4.15"
  },
  "devDependencies": {},
  "homepage": "http://marquee.by",
  "bugs": "https://github.com/marquee/marquee-static-sdk",
  "repository": {
    "type": "git",
    "url": "git://github.com/marquee/marquee-static-sdk.git"
  },
